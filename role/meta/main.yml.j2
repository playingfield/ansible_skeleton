---
# https://docs.ansible.com/ansible/latest/galaxy/dev_guide.html
galaxy_info:
  author: {{ lookup('env','USER') }}
  description: Ansible role {{ role_name }}
  company: Company (optional)
  role_name: {{ role_name }}
  namespace: your_namespace
  license: MIT
  min_ansible_version: "2.10"
  platforms:
    - name: EL
      versions:
        - "8"

  galaxy_tags:
    - rhel
    # NOTE: A tag is limited to a single word comprised of alphanumeric
    # characters. Maximum 20 tags per role.

dependencies: []
# List your role dependencies here, one per line.
# Be sure to remove the '[]' above,
# if you add dependencies to this list.
